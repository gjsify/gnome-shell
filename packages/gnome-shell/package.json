    "generate:exports": "node ./scripts/generate-exports.js && yarn install",
    "generate:files": "node ./scripts/generate-files.js",
    "generate:index": "node ./scripts/generate-index.js",
    "generate:types": "node ./scripts/generate-types.js"
  },
